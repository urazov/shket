<div class='row' style='margin-top: 20px;'>
    <div class='col-xs-12 table-responsive'>
        <table class= 'table table-striped table-simple table-cabinet table-small'>
            <thead>
                <th>Дата прохода</th>
                <th>Время прохода</th>
                <th>Направление прохода</th>
            </thead>
            <tbody>
            {% for str in result %}
                <tr>
                    <td class='center'>{{ str[0] }}</td>
                    <td class='center'>{{ str[1] }}</td>
                    <td class='center'>{{ str[2] }}</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>
</div>